{
  "name": "Screw'd Up Click",
  "description": "Screw your music with the click of a button. Chops not included.",
  "version": "1.0",
   "manifest_version": 2,
  "icons": {"128": "Screw_128.png", "16":"Screw_19.png"},
  "browser_action":{
    "default_icon": "Screw_19.png",
    "default_popup": "index.html"
  },

"permissions": ["activeTab"],
"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self' 'unsafe-inline' ",
"content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "jungle.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
]
}